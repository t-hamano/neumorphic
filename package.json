{
	"name": "neumorphic",
	"version": "1.2.1",
	"author": "Tetsuaki Hamano",
	"license": "GPL-2.0+",
	"scripts": {
		"wp-env": "wp-env",
		"open": "wp-env start && opener http://localhost:8888",
		"stop": "wp-env stop",
		"start": "node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js assets/sass/ --output assets/css/ --watch --source-map true",
		"build": "npm-run-all -s build:clean build:css:compressed build:css:rename build:css:expanded",
		"build:clean": "rimraf assets/css",
		"build:css:expanded": "node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js assets/sass/ --output assets/css/ --output-style expanded --indent-width 2 --indent-type tab",
		"build:css:compressed": "node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js assets/sass/ --output assets/css/ --output-style compressed",
		"build:css:rename": "renamer -f \"/.css$/\" -r \".min.css\" \"assets/css/*\"",
		"release": "npm-run-all -s release:clean:dir release:clean:zip release:mkdir release:copy release:archive release:clean:dir",
		"release:clean:dir": "cross-conf-env rimraf ./npm_package_name",
		"release:clean:zip": "cross-conf-env rimraf ./*.zip",
		"release:copy": "cross-conf-env copyfiles -e \"./node_modules/**/*\" -e \"./vendor/**/*\" -e \"./*.json\" -e \"./*.xml\" -e \"./*.lock\" ./**/* ./npm_package_name",
		"release:mkdir": "cross-conf-env mkdirp npm_package_name",
		"release:archive": "cross-conf-env bestzip ./npm_package_name-npm_package_version.zip ./npm_package_name"
	},
	"devDependencies": {
		"@wordpress/env": "^2.1.0",
		"bestzip": "^2.1.7",
		"copyfiles": "^2.4.1",
		"cross-conf-env": "^1.2.1",
		"eslint": "^7.15.0",
		"eslint-config-prettier": "^7.0.0",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-plugin-prettier": "^3.3.0",
		"mkdirp": "^1.0.4",
		"node-sass": "^5.0.0",
		"node-sass-glob-importer": "^5.3.2",
		"npm-run-all": "^4.1.5",
		"opener": "^1.5.2",
		"renamer": "^2.0.1",
		"rimraf": "^3.0.2",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-recommended-scss": "^4.2.0",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-scss": "^3.18.0"
	},
	"dependencies": {}
}
